{
  "name": "fastify-template",
  "version": "1.0.0",
  "description": "A Fastify template using Typescript",
  "main": "src/server.ts",
  "scripts": {
    "dev": "npx ts-node-dev --respawn --transpileOnly -r dotenv/config ./src/server.ts",
    "build": "tsc -b",
    "clean": "rm -rf ./build/*",
    "start": "node ./build/server.js"
  },
  "keywords": [
    "template",
    "fastify",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.7.32",
    "@types/node": "^14.0.23",
    "@types/pino": "^6.3.0",
    "dotenv": "^8.2.0",
    "pino-pretty": "^4.1.0",
    "ts-node-dev": "^1.0.0-pre.43",
    "tslint-config-standard": "^9.0.0",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "ajv": "^6.12.3",
    "axios": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "fastify": "^3.29.4",
    "fastify-cors": "^4.1.0",
    "fastify-helmet": "^4.0.2",
    "fastify-oas": "^2.7.1",
    "fastify-plugin": "^2.0.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.25",
    "pino": "^6.4.0"
  }
}
