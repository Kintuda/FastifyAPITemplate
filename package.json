{
  "name": "fastify-template",
  "version": "1.0.0",
  "description": "A Fastify template using Typescript",
  "main": "src/server.ts",
  "scripts": {
    "dev": "npx ts-node-dev --respawn --transpileOnly -r dotenv/config ./src/server.ts",
    "build": "tsc -b",
    "clean": "rm -rf ./build/*",
    "start": "node ./build/server.js"
  },
  "keywords": [
    "template",
    "fastify",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/mongoose": "^5.5.19",
    "@types/node": "^12.7.11",
    "@types/pino": "^5.8.10",
    "pino-pretty": "^3.2.1",
    "ts-node-dev": "^1.0.0-pre.43",
    "tslint-config-standard": "^8.0.1",
    "dotenv": "^8.2.0",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "ajv": "^6.10.2",
    "fastify": "^2.10.0",
    "fastify-cors": "^2.2.0",
    "fastify-helmet": "^3.0.2",
    "fastify-plugin": "^1.6.0",
    "mongoose": "^5.7.10",
    "pino": "^5.13.6"
  }
}
