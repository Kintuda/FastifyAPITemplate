{
  "name": "fastify-template",
  "version": "1.0.0",
  "description": "A Fastify template using Typescript",
  "main": "src/server.ts",
  "scripts": {
    "dev": "npx ts-node-dev --respawn --transpileOnly -r dotenv/config ./src/server.ts",
    "build": "tsc -b",
    "clean": "rm -rf ./build/*",
    "start": "node ./build/server.js"
  },
  "keywords": [
    "template",
    "fastify",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/mongoose": "^5.5.36",
    "@types/node": "^13.1.2",
    "@types/pino": "^5.15.1",
    "dotenv": "^8.2.0",
    "pino-pretty": "^3.5.0",
    "ts-node-dev": "^1.0.0-pre.43",
    "tslint-config-standard": "^9.0.0",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "ajv": "^6.10.2",
    "axios": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "fastify": "^2.11.0",
    "fastify-cors": "^3.0.0",
    "fastify-helmet": "^3.0.2",
    "fastify-oas": "^2.5.0",
    "fastify-plugin": "^1.6.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.8.3",
    "pino": "^5.15.0"
  }
}
